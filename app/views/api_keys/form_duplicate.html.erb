        <button class=" btn btn-primary" id="btn_add_input">Add String</button>
    </div>
    <div class="col-md-3">
        <button class=" btn btn-success" id="btn_show_duplicate">Show duplicate</button>
    </div>

    <div class="col-md-2">
        <button class="btn btn-danger" id="btn_reset_input">Reset</button>
    </div>
</div>

